<tr>
  <td>
    <%= project_table_scope.name %>
  </td>

  <td>
    <%= project_table_scope.table %>
  </td>

  <td>
    <div class="btn-group">
      <%= link_to project_table_scope, class: "btn btn-sm btn-outline-secondary" do %>
        <i class="fas fa-search-plus fa-fw"></i>
      <% end %>

      <%= link_to edit_project_table_scope_url(project_table_scope), class: "btn btn-sm btn-outline-secondary" do %>
        <i class="fas fa-edit fa-fw"></i>
      <% end %>

      <%= link_to project_table_scope, method: :delete, data: { confirm: "Do you really want to delete this project_table_scope?" }, class: "btn btn-sm btn-outline-secondary" do %>
        <i class="fas fa-trash-alt fa-fw"></i>
      <% end %>
    </div>
  </td>
</tr>
