<tr>
  <td>
    <%= project_table_column.name %>
  </td>

  <td>
    <%= project_table_column.primary_descriptor %>
  </td>

  <td>
    <%= project_table_column.type %>
  </td>

  <td>
    <%= project_table_column.table %>
  </td>

  <td>
    <div class="btn-group">
      <%= link_to project_table_column, class: "btn btn-sm btn-outline-secondary" do %>
        <i class="fas fa-search-plus fa-fw"></i>
      <% end %>

      <%= link_to edit_project_table_column_url(project_table_column), class: "btn btn-sm btn-outline-secondary" do %>
        <i class="fas fa-edit fa-fw"></i>
      <% end %>

      <%= link_to project_table_column, method: :delete, data: { confirm: "Do you really want to delete this project_table_column?" }, class: "btn btn-sm btn-outline-secondary" do %>
        <i class="fas fa-trash-alt fa-fw"></i>
      <% end %>
    </div>
  </td>
</tr>
