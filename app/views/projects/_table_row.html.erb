<tr>
  <td>
    <%= project.user %>
  </td>

  <td>
    <%= project.codename %>
  </td>

  <td>
    <%= project.parent %>
  </td>

  <td>
    <%= project.public %>
  </td>

  <td>
    <%= project.fork_order %>
  </td>

  <td>
    <div class="btn-group">
      <%= link_to project, class: "btn btn-sm btn-outline-secondary" do %>
        <i class="fas fa-search-plus fa-fw"></i>
      <% end %>

      <%= link_to edit_project_url(project), class: "btn btn-sm btn-outline-secondary" do %>
        <i class="fas fa-edit fa-fw"></i>
      <% end %>

      <%= link_to project, method: :delete, data: { confirm: "Do you really want to delete this project?" }, class: "btn btn-sm btn-outline-secondary" do %>
        <i class="fas fa-trash-alt fa-fw"></i>
      <% end %>
    </div>
  </td>
</tr>
